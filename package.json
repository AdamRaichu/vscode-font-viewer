{
  "name": "font-viewer",
  "displayName": "Font Preview",
  "description": "An extension which allows you to preview fonts.",
  "publisher": "adamraichu",
  "icon": "file-font-icon.png",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-font-viewer"
  },
  "version": "1.0.2",
  "browser": "./dist/main.js",
  "main": "./dist/main.js",
  "activationEvents": [
    "onCustomEditor:fontViewer.FontEdit"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "fontViewer.FontEdit",
        "displayName": "Font Preview",
        "selector": [
          {
            "filenamePattern": "*.otf"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "build": "webpack"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}